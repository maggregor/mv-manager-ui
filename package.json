{
  "name": "achilio-mv-manager-ui",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev --port 8081 --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --write \"src/**/*.{vue,js,jsx,ts,tsx,scss,less}\"",
    "deploy": "cd .cloudflare/ && wrangler publish",
    "build-demo": "VUE_APP_AUTHENTICATED=true vue-cli-service build",
    "build-deploy": "yarn vb-full && yarn build-demo && yarn deploy && yarn vb-seed",
    "vb": "node ./scripts/vb.js",
    "vb-full": "REDIRECT_URL=/dashboard/alpha node ./scripts/generate.js --config=config-full.json",
    "vb-seed": "node ./scripts/generate.js --config=config-seed.json"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.1.0-rc.6",
    "axios": "^0.26.1",
    "bootstrap": "^4.5.3",
    "chalk": "^4.1.0",
    "charts.css": "^0.9.0",
    "cli-select": "^1.1.2",
    "he-tree-vue": "3",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "nprogress": "^0.2.0",
    "pretty-bytes": "^5.6.0",
    "qs": "^6.9.6",
    "readline-sync": "^1.4.10",
    "register-service-worker": "^1.7.1",
    "store": "^2.0.12",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0-rc.5",
    "vue-router": "^4.0.13",
    "vue3-perfect-scrollbar": "^1.5.5",
    "vue3-popper": "^1.4.2",
    "vue3-quill": "^0.2.6",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {    
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "vue-stripe-js": "^1.0.0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
